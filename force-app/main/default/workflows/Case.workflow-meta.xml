    </rules>
    <rules>
        <fullName>CASE_WF06 Clôture automatique 15 jours</fullName>
        <active>true</active>
        <booleanFilter>2 AND 1</booleanFilter>
        <criteriaItems>
            <field>Case.Status</field>
            <operation>equals</operation>
            <value>EnAttenteRetourClient</value>
        </criteriaItems>
        <criteriaItems>
            <field>User.ByPassWorkflow__c</field>
            <operation>equals</operation>
            <value>False</value>
        </criteriaItems>
